<% provide(:title, 'All users') %>
<h1>All users</h1>

<div class="row">
  <div class="span5">
    <%= form_tag users_path, method: 'get' do %>
      <%= text_field_tag :search, params[:search] %>
      <%= submit_tag "Search", name: nil, class: "btn btn-large btn-primary" %>
    <% end %>
  </div>
</div>

<%= will_paginate %>

<ul class="users">
  <%= render @users %>
</ul>

<%= will_paginate %>
